{
  "general": {
    "location": "0.0.0.0:8081",
    "logger": {
      "debug": true
    }
  },
  "service": {
    "location": "localhost:8080",
    "title": "Expand Seed Set",
    "version": "0.1",
    "author": "Michael Yu",
    "email": "mikeyu@ucsd.edu",
    "website": "",
    "repository": "http://github.com/michaelkyu/ontology",
    "description": "Identifies genes that are highly related to a seed set",
    "license": "MIT",
    "language": "Python",
    "parameters": [
      {
            "name": "seed",
            "default": "None",
            "description": "Seed genes"
      },
      {
            "name": "ndex_uuid",
            "default": "None",
            "description": "asdf"
      },
      {
            "name": "ndex_user",
            "default": "None",
            "description": "asf"
      },
    {
            "name": "ndex_pass",
            "default": "None",
            "description": "asdf"
    },
    {
            "name": "input_fmt",
            "default": "None",
            "description": "asdf"
    },
    {
            "name": "output_fmt",
            "default": "None",
            "description": "asdf"
    },        
    {
            "name": "agg",
            "default": "None",
            "description": "asdf"
    },
    {
            "name": "min_sim",
            "default": "-1000000",
            "description": "asdf",
        "type" : "number"
    },
    {
            "name": "filter_perc",
            "default": "0",
            "description": "asdf",
        "type" : "number"
    },    
        {
            "name": "seed_perc",
            "default": "0",
            "description": "asdf",
        "type" : "number"
    },
    {
            "name": "agg_perc",
            "default": "0",
            "description": "asdf",
        "type" : "number"
    },
    {
            "name": "expand_size",
            "default": "500",
            "description": "asdf",
        "type" : "number"
    },
      {
        "name": "normalize_laplacian",
        "default": "False",
        "description": "If True, will create a normalized laplacian matrix for diffusion",
        "type": "boolean"
      },
      {
        "name": "input_attribute_name",
        "default": "diffusion_input",
        "description": "The key diffusion will use to search for heats in the node attributes with"
      },
      {
        "name": "output_attribute_name",
        "default": "diffusion_output",
        "description": "Will be the prefix of the _rank and _heat attriubtes created by diffusion"
      }
    ],
    "input": [
      {
        "label": "Input",
        "description": "An input network with heat values attached to nodes",
        "aspects": ["nodes", "edges", "nodeAttributes"]
      }
    ],
    "singletonInput": true,
    "output": [
      {
        "label": "Output",
        "description": "An output network with new heats and a rank attribute created by diffusion",
        "aspects": ["nodes", "edges", "nodeAttributes"]
      }
    ],
    "singletonOutput": true
  }
}